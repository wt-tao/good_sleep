<!--pages/uesr/uesr.wxml-->
<image src='../../img/tc_5_kb.png' style='width:100%' mode='widthFix'/>

<view class='uesr'>
    <view class='uesr_info flex'>
          <view class='info_img'></view>
          <view class='info_'>
              <view class='name'>用户昵称</view>
              <view class='pnone'>手机号:12345678915</view>
          </view>
          <view class='end flex'>
            <image src='../../img/tuichu.png' style='width:40rpx;margin-top:20rpx;margin-left:20rpx' mode='widthFix'/>
            <view class='tuic'>退出登录</view>
          </view>
    </view>

    <view class='bq flex'>
        <view class='bq_ flex' bindtap='address'>
            <image src='../../img/daohangdizhiweizhi.png' style='width:40rpx;margin-top:10rpx;margin-left:50rpx;' mode='widthFix'/>
            <view class='bq_text'>收货地址</view>
        </view>
        <view class='bq_ flex' style='border:none' bindtap='my_sc'>
            <image src='../../img/ysc (2).png' style='width:40rpx;margin-top:10rpx;margin-left:50rpx;' mode='widthFix'/>
            <view class='bq_text'>我的收藏</view>
        </view>
    </view>
</view>

<view class='div flex'>
    <view class='div_' bindtap='order'>
        <image src='../../img/kdysm (4).png' style='width:30%;margin-left:35%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>全部订单</view>
        <view class='div_number'>1</view>
    </view>
    <view class='div_'>
        <image src='../../img/kdysm (1).png' style='width:40%;margin-left:30%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>待确认</view>
        <view class='div_number'>1</view>
    </view>
    <view class='div_'>
        <image src='../../img/kdysm (3).png' style='width:27%;margin-left:37.5%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>已完成</view>
        <view class='div_number'>1</view>
    </view>
    <view class='div_'>
        <image src='../../img/kdysm (2).png' style='width:40%;margin-left:30%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>已取消</view>
        <view class='div_number'>1</view>
    </view>
</view>

<view class='money'>
    <view class='money_tit flex'>
        <view class='icon'></view>
        <view class='icon_name'>可提现金额</view>
        <image src='../../img/chakan.png' style='width:40rpx;margin-top:25rpx;' mode='widthFix'/>
        <view class='img_text' bindtap='withdrawals_record'>查看提现记录</view>
    </view>
    <view class='money_nr flex' >
        <view class='money_num'>200.00<text style='font-size:26rpx;'> 元</text></view>
        <view class='money_tx' bindtap='banlance'>申请提现</view>
    </view>
</view>


<view class='white'>

    <view class='white_'  >
    
            <view class='yaoq' style='height:300rpx;' bindtap='order_detail'>
                <view class='yaoq_ti flex'>
                    <view class='yaoq_zt' style='color:#22C891'>已完成</view>
                    <view class='yaoq_time'>2018-12-12 14:00:00</view>
                </view>
                <view class='yaoq1_ flex'>
                   <view class='order_img'></view>
                   <view class='order_info'>
                      <view class='order_name'>泰国纯天然乳胶枕头狼牙乳胶枕天然按摩枕记忆枕水电费水电费是大法官</view>
                      <view class='order_card'>订单号：3163541135415616</view>
                   </view>
                </view>
                 <view class='order_buttom flex'>
                      <view class='order_money'>￥168.00</view>
                      <view class='order_button flex'>
                          <view class='anniu'  bindtap='return_goods'>申请退货</view>
                          <view class='anniu' style='background-color:#33CC99;color:white;border:none' bindtap='see_wl'>查看物流</view>
                      </view>
                  </view>
                  <image src='../../img/tc11.png' class='yqo_img' mode='widthFix'/>
            </view>

            <view class='yaoq'>
                <view class='yaoq_ti flex'>
                    <view class='yaoq_zt' style='color:#E31111'>已取消</view>
                    <view class='yaoq_time'>2018-12-12 14:00:00</view>
                </view>
                <view class='yaoq1_ flex'>
                   <view class='order_img'></view>
                   <view class='order_info'>
                      <view class='order_name'>泰国纯天然乳胶枕头狼牙乳胶枕天然按摩枕记忆枕水电费水电费是大法官</view>
                      <view class='order_card'>订单号：3163541135415616</view>
                   </view>
                </view>
                 <view class='order_buttom flex'>
                      <view class='order_money'>￥168.00</view>
                      <view class='order_button flex'>
                          <view class='anniu'>申请退货</view>
                      </view>
                  </view>
            </view>

            <view class='yaoq'>
                <view class='yaoq_ti flex'>
                    <view class='yaoq_zt' style='color:#0084FF'>待确认</view>
                    <view class='yaoq_time'>2018-12-12 14:00:00</view>
                </view>
                <view class='yaoq1_ flex'>
                   <view class='order_img'></view>
                   <view class='order_info'>
                      <view class='order_name'>泰国纯天然乳胶枕头狼牙乳胶枕天然按摩枕记忆枕水电费水电费是大法官</view>
                      <view class='order_card'>订单号：3163541135415616</view>
                   </view>
                </view>
                 <view class='order_buttom flex'>
                      <view class='order_money'>￥168.00</view>
                      <view class='order_button flex'>
                          <view class='anniu'>申请退货</view>
                      </view>
                  </view>
            </view>
        <view style='height:20rpx;'></view>
    </view>
</view>

<!-- 弹窗 -->
<view hidden='{{sort}}'>
<view class="bg" bindtap='bindSorttap'></view>
<view class="show" >
      <image src='../../img/gb.png' style='width:30rpx;margin-top:20rpx;margin-left:550rpx;' mode='widthFix' bindtap='bindSorttap'/>
      <view class='show_tit'>顺丰快递</view>
      <view class='show_text'>快递单号：132168515263165416</view>
</view>
</view>